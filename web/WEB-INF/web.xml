         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <display-name>Mini User Data Cache Updater</display-name>

    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>
            web/WEB-INF/configFiles.xml
            web/WEB-INF/datalogging.xml
            web/WEB-INF/miniudcu.xml
        </param-value>
    </context-param>
</web-app>